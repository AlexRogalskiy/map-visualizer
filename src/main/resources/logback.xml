<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="C" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d %-5p %c - %m%n</pattern>
        </encoder>
    </appender>

    <root level="WARN">
        <appender-ref ref="C" />
    </root>
</configuration>
